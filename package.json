{
  "name": "yul-yort-admin",
  "version": "1.1.1",
  "homepage": ".",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "serve": "npm run serve-build && node server.js",
    "serve-build": "cross-env REACT_APP_BUILD_MODE=serve BUILD_PATH='serve-build' react-scripts build",
    "gh-pages-build": "cross-env REACT_APP_BUILD_MODE=gh-pages BUILD_PATH='gh-pages' react-scripts build",
    "build": "react-scripts build",
    "module": "node createModule.js",
    "test": "react-scripts test",
    "testCoverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "madge": "madge --extensions ts,tsx --dot ./src > graph.gv",
    "get-css-vars": "rm -rf ./public/styles/mui-variables.css && node getCSSVariablesFromTheme.js && prettier --write ./public/styles/mui-variables.css"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.1",
    "@mui/material": "5.14.1",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "mobx": "^6.3.3",
    "mobx-react-lite": "4.0.3",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.2",
    "react-router5": "^8.0.1",
    "react-scripts": "5.0.1",
    "router5": "^8.0.1",
    "router5-plugin-browser": "^8.0.1",
    "url-join": "^5.0.0",
    "web-vitals": "3.4.0",
    "workbox-background-sync": "7.0.0",
    "workbox-broadcast-update": "7.0.0",
    "workbox-cacheable-response": "7.0.0",
    "workbox-core": "7.0.0",
    "workbox-expiration": "7.0.0",
    "workbox-google-analytics": "7.0.0",
    "workbox-navigation-preload": "7.0.0",
    "workbox-precaching": "7.0.0",
    "workbox-range-requests": "7.0.0",
    "workbox-routing": "7.0.0",
    "workbox-strategies": "7.0.0",
    "workbox-streams": "7.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^29.5.0",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^12.20.25",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "husky": "^7.0.0",
    "jest": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^12.4.0",
    "lodash": "4.17.21",
    "madge": "^5.0.1",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.52.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "npm run lint:write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      ",/node_modules/(?!url-join)"
    ]
  }
}
