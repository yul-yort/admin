{
  "name": "yul-yort-admin",
  "version": "1.1.1",
  "homepage": ".",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "serve": "npm run serve-build && node server.js",
    "serve-build": "cross-env REACT_APP_BUILD_MODE=serve BUILD_PATH='serve-build' react-scripts build",
    "gh-pages-build": "cross-env REACT_APP_BUILD_MODE=gh-pages BUILD_PATH='gh-pages' react-scripts build",
    "build": "react-scripts build",
    "module": "node createModule.js",
    "test": "react-scripts test",
    "testCoverage": "npm test --coverage",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "madge": "madge --extensions ts,tsx --dot ./src > graph.gv",
    "get-css-vars": "rm -rf ./public/styles/mui-variables.css && node getCSSVariablesFromTheme.js && prettier --write ./public/styles/mui-variables.css"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.25",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.39.1",
    "react-router5": "^8.0.1",
    "react-scripts": "5.0.1",
    "router5": "^8.0.1",
    "router5-plugin-browser": "^8.0.1",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.0",
    "madge": "^5.0.1",
    "msw": "^0.35.0",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.52.2",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "npm run lint:write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
